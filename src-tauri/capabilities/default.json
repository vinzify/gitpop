{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-show",
    "core:window:allow-close",
    "opener:default",
    "store:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-load",
    "store:allow-save",
    "process:default",
    "process:allow-exit"
  ]
}